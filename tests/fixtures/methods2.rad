
type Thing:
  x : int
  y : int

fn foo(thing: Thing, z: int) @method:
  print(thing.x + z)

fn bam(thing: Thing) @method:
  print(thing.x)

fn main():

  let thing = Thing(0, 0)

  thing.x = 1
  thing.y = 10
  print(thing.x + thing.y)

  thing.foo(200)
  thing.bam

  const fam = {|x| x + 100}

  let x = 2.fam
