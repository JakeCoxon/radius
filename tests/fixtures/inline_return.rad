
fn bar!(T)() -> int @inline:
  let x = 43
  return x + 32
  
fn foo(i: int) -> int @inline:
  if i < 3:
    return bar!(int)()
  else:
    return bar!(int)()
  
fn baz(i: int) -> int @inline:
  if i > 3:
    return foo(i)
  32 # Implicit return must set breakExprBinding

fn main():
  print("Before")
  let z = bar!(123)()
  print("After")

  print(z)

  let f = foo(2)
  print(f)
  
  print(baz(1))