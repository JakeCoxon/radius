
fn iterate!(f, T)(lst: List!T) @inline @method:
  i := 0
  while i < lst.length:
    f(lst[i])
    i += 1

fn main():
  lst := [1,2,3]
  print(lst.length)
  print(lst[0])

  # All three should produce the same AST
  
  i := 0
  while i < lst.length:
    print(lst[i])
    i += 1

  f :: |x| print(x)
  lst.iterate!(f, int)()

  for x in lst:
    print(x)
