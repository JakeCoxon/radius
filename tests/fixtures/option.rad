import compiler for Option, Some, None

fn is_some!(T)(x: Option!T) -> bool @inline:
  if x is Some:
    return true
  return false

fn main():
  x := None!int()
  y := Some!int(10)

  y = Some!int(32)

  if y is Some:
    print(y.value + 123)
  else:
    print("None")

  print(x)
  print(y)

  z := x orelse 69
  w := y orelse 100
  print(z + w)

  if x.is_some:
    print(x)
  else:
    print("None", x)

  # print(x)
