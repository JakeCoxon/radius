import compiler for Option, Some, None

fn is_some!(T)(x: Option!T) -> bool @inline:
  if x is Some:
    return true
  return false

fn main():
  x := None!int()
  y := Some!int(10)

  x = Some!int(32)

  if x is Some:
    print(x.value + 123)
  else:
    print("None")

  print(x)
  print(y)

  if x.is_some:
    print(x)
  else:
    print("None", x)

  # print(x)
